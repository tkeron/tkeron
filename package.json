{
  "name": "tkeron",
  "version": "4.0.0-beta.10",
  "description": "CLI tool for backend-driven frontend development with TypeScript.",
  "type": "module",
  "bin": {
    "tkeron": "./index.ts",
    "tk": "./index.ts",
    "tkeron-mcp": "./mcp-server.ts"
  },
  "scripts": {
    "tk": "bun index.ts ",
    "test": "bun test --concurrent",
    "lint": "prettier --write .",
    "sample1": "bun . build examples/basic_build/src",
    "sample2": "bun . build examples/with_assets/src",
    "sample3": "bun . build examples/with_pre/src",
    "sample4": "bun . build examples/with_com_html_priority/src",
    "sample5": "bun . build examples/with_com_ts/src",
    "sample6": "bun . build examples/with_com_ts_priority/src",
    "sample7": "bun . build examples/with_com_mixed_priority/src",
    "build_examples": "bun run sample1 && bun run sample2 && bun run sample3 && bun run sample4 && bun run sample5 && bun run sample6 && bun run sample7"
  },
  "author": "tkeron",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@tkeron/commands": "0.3.1",
    "@tkeron/html-parser": "^0.1.5",
    "@tkeron/tools": "^0.2.2"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "cli",
    "build-tool",
    "frontend",
    "typescript",
    "pre-rendering",
    "static-site-generator",
    "backend-driven",
    "mcp",
    "model-context-protocol",
    "ai-integration"
  ],
  "repository": {
    "url": "git@github.com:tkeron/tkeron.git"
  }
}
